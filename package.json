{
  "name": "api-first-auto-tuner",
  "version": "1.0.0",
  "description": "Automatically discovers internal API endpoints and generates complete API documentation in OpenAPI, Postman, cURL, and Insomnia formats. The only tool that saves weeks of manual API documentation work.",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsc --watch",
    "test": "npm run build && node dist/test-main.js",
    "test:watch": "npm run dev & node --watch dist/test-main.js",
    "verify": "node verify-build.js"
  },
  "keywords": [
    "apify",
    "crawlee",
    "scraping",
    "api-discovery",
    "api-documentation",
    "openapi",
    "postman",
    "playwright",
    "http-crawler",
    "automation",
    "web-scraping"
  ],
  "author": "irun2themoney",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/irun2themoney/api-first-auto-tuner.git"
  },
  "dependencies": {
    "crawlee": "^3.9.0",
    "playwright": "^1.48.0",
    "apify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.5.0"
  }
}

