{
  "actorSpecification": 1,
  "title": "APX Toolkit Dataset",
  "description": "Dataset schema for APX Toolkit output containing discovered APIs, generated code, documentation, types, tests, SDKs, and extracted data",
  "fields": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": true
  },
  "views": {
    "discoveredApis": {
      "title": "Discovered APIs üì°",
      "description": "API endpoint summaries with metadata",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "url",
            "type": "string"
          },
          {
            "name": "method",
            "type": "string"
          },
          {
            "name": "statusCode",
            "type": "number"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "discoveredApi"
      }
    },
    "extractedData": {
      "title": "Extracted Data üìä",
      "description": "All extracted data items from discovered APIs",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "data",
            "type": "object"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "extractedData"
      }
    },
    "codeSnippets": {
      "title": "Code Snippets üíª",
      "description": "Ready-to-use code snippets in 12 programming languages",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "language",
            "type": "string"
          },
          {
            "name": "code",
            "type": "string"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "codeSnippet"
      }
    },
    "typescriptTypes": {
      "title": "TypeScript Types üìò",
      "description": "Complete TypeScript type definitions for all discovered APIs",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "types",
            "type": "string"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "typescriptTypes"
      }
    },
    "apiDocumentation": {
      "title": "API Documentation üìö",
      "description": "API documentation in multiple formats (OpenAPI, Postman, cURL, Insomnia)",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "format",
            "type": "string"
          },
          {
            "name": "content",
            "type": "string"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "apiDocumentation"
      }
    },
    "testSuites": {
      "title": "Test Suites üß™",
      "description": "Ready-to-run test suites in 5 testing frameworks",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "framework",
            "type": "string"
          },
          {
            "name": "tests",
            "type": "string"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "testSuite"
      }
    },
    "sdkPackages": {
      "title": "SDK Packages üì¶",
      "description": "Complete SDK packages for TypeScript, Python, and Go",
      "display": {
        "type": "table"
      },
      "transformation": {
        "fields": [
          {
            "name": "language",
            "type": "string"
          },
          {
            "name": "package",
            "type": "string"
          }
        ]
      },
      "filter": {
        "field": "type",
        "value": "sdkPackage"
      }
