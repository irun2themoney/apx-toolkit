        "fields": ["apiUrl", "sourceUrl", "statusCode", "timestamp", "extractedAt", "page", "offset", "method"]
      },
      "display": {
        "component": "table",
        "properties": {
          "apiUrl": {
            "label": "API URL",
            "format": "link"
          },
          "sourceUrl": {
            "label": "Source URL",
            "format": "link"
          },
          "statusCode": {
            "label": "Status Code",
            "format": "number"
          },
          "timestamp": {
            "label": "Timestamp",
